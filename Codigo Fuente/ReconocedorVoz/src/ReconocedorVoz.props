#options.reconocedorVoz.choose-file.title=Choose a notepad file
#options.reconocedorVoz.choose-font=Font:
#options.reconocedorVoz.show-filepath.title=Display notepad file path

# window title
reconocedorVoz.title=Reconocedor Voz

# default settings
options.reconocedorVoz.show-filepath=true
options.reconocedorVoz.font=Monospaced
options.reconocedorVoz.fontstyle=0
options.reconocedorVoz.fontsize=14

# Setting not defined but supplied for completeness
options.reconocedorVoz.filepath=
